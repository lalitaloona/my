{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "@app/*": ["src/*"],
      "@config/*": ["src/config/*"],
      "@components/*": ["src/components/*"],
      "@redux/*": ["src/redux/*"],
      "@static/*": ["src/static/*"],
      "@apollo-client/*": ["src/components/Apollo/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
